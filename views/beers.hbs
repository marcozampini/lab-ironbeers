<main>
  <section class='beers'>
    {{#each beersFromApi}}
      <article class='beer'>
        <picture>
          <img src='{{this.image_url}}' alt='{{this.name}}' />
        </picture>
        <h3>{{this.name}}</h3>
        <h4>{{this.tagline}}</h4>
        <p>
          {{this.description}}
        </p>
      </article>
    {{/each}}
  </section>
</main>